        class="list-group-item list-group-item-action list-group-item-dark p-3 ps-5 <%= (typeof activePage !== 'undefined' && activePage === 'add_career') ? 'active' : '' %>"
      >
        <i class="fas fa-plus-circle me-2"></i>Add Job Posting
      </a>
      <a
        href="/admin/careers"
        class="list-group-item list-group-item-action list-group-item-dark p-3 ps-5 <%= (typeof activePage !== 'undefined' && activePage === 'view_careers') ? 'active' : '' %>"
      >
        <i class="fas fa-list-alt me-2"></i>View Job Postings
