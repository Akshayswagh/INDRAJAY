      href="#careManagementSubmenu"
      role="button"
      aria-expanded="<%= (typeof activePage !== 'undefined' && (activePage === 'add_care_management' || activePage === 'view_care_management' || activePage === 'edit_care_management')) ? 'true' : 'false' %>"
      aria-controls="careManagementSubmenu"
    >
      <span><i class="fas fa-heartbeat me-2"></i>Care & Management</span> <%#
      Icon for care/health %>
      <i class="fas fa-chevron-down"></i>
    </a>
    <div
      class="collapse <%= (typeof activePage !== 'undefined' && (activePage === 'add_care_management' || activePage === 'view_care_management' || activePage === 'edit_care_management')) ? 'show' : '' %>"
      id="careManagementSubmenu"
    >
      <a
        href="/admin/care-management/add"
        class="list-group-item list-group-item-action list-group-item-dark p-3 ps-5 <%= (typeof activePage !== 'undefined' && activePage === 'add_care_management') ? 'active' : '' %>"
