      <i class="fas fa-envelope-open-text me-2"></i>Manage Enquiries <%# Or fas
      fa-question-circle %>
    </a>


    <!-- Trusted Buyers Submenu -->
    <a
      class="list-group-item list-group-item-action list-group-item-dark p-3 d-flex justify-content-between align-items-center"
      data-bs-toggle="collapse"
