      <i class="fas fa-chevron-down"></i>
    </a>
    <div
      class="collapse <%= (typeof activePage !== 'undefined' && (activePage === 'add_trusted_buyer' || activePage === 'view_trusted_buyers' || activePage === 'edit_trusted_buyer')) ? 'show' : '' %>"
