<div class="bg-dark border-end" id="sidebar-wrapper">
  <div class="sidebar-heading border-bottom bg-dark text-white">
    <i class="fas fa-cogs me-2"></i>Admin Panel
  </div>
  <div class="list-group list-group-flush">
    <a
      href="/admin"
      class="list-group-item list-group-item-action list-group-item-dark p-3 <%= (typeof activePage !== 'undefined' && activePage === 'dashboard') ? 'active' : '' %>"
    >
      <i class="fas fa-tachometer-alt me-2"></i>Dashboard
    </a>

    <!-- Users Submenu -->
    <a
      class="list-group-item list-group-item-action list-group-item-dark p-3 d-flex justify-content-between align-items-center"
      data-bs-toggle="collapse"
      href="#userSubmenu"
      role="button"
      aria-expanded="<%= (typeof activePage !== 'undefined' && (activePage === 'view_users' || activePage === 'view_vendors' || activePage === 'view_admins')) ? 'true' : 'false' %>"
      aria-controls="userSubmenu"
    >
      <span><i class="fas fa-users me-2"></i>Users</span>
      <i class="fas fa-chevron-down"></i>
    </a>
    <div
      class="collapse <%= (typeof activePage !== 'undefined' && (activePage === 'view_users' || activePage === 'view_vendors' || activePage === 'view_admins')) ? 'show' : '' %>"
      id="userSubmenu"
    >
      <a
        href="/admin/users"
        class="list-group-item list-group-item-action list-group-item-dark p-3 ps-5 <%= (typeof activePage !== 'undefined' && activePage === 'view_users') ? 'active' : '' %>"
      >
        <i class="fas fa-user-friends me-2"></i>View Users
      </a>
      <a
        href="/admin/vendors"
        class="list-group-item list-group-item-action list-group-item-dark p-3 ps-5 <%= (typeof activePage !== 'undefined' && activePage === 'view_vendors') ? 'active' : '' %>"
      >
        <i class="fas fa-store me-2"></i>View Vendors
      </a>
      <a
        href="/admin/admins"
        class="list-group-item list-group-item-action list-group-item-dark p-3 ps-5 <%= (typeof activePage !== 'undefined' && activePage === 'view_admins') ? 'active' : '' %>"
      >
        <i class="fas fa-user-shield me-2"></i>View Admins
      </a>
    </div>

    <!-- Export Submenu -->
    <a
      class="list-group-item list-group-item-action list-group-item-dark p-3 d-flex justify-content-between align-items-center"
      data-bs-toggle="collapse"
      href="#exportSubmenu"
      role="button"
      aria-expanded="<%= (typeof activePage !== 'undefined' && (activePage === 'add_export' || activePage === 'view_exports' || activePage === 'edit_export')) ? 'true' : 'false' %>"
      aria-controls="exportSubmenu"
    >
      <span><i class="fas fa-file-export me-2"></i>Exports</span>
      <i class="fas fa-chevron-down"></i>
    </a>
    <div
      class="collapse <%= (typeof activePage !== 'undefined' && (activePage === 'add_export' || activePage === 'view_exports' || activePage === 'edit_export')) ? 'show' : '' %>"
      id="exportSubmenu"
    >
      <a
        href="/admin/exports/add"
        class="list-group-item list-group-item-action list-group-item-dark p-3 ps-5 <%= (typeof activePage !== 'undefined' && activePage === 'add_export') ? 'active' : '' %>"
      >
        <i class="fas fa-plus-circle me-2"></i>Add Export
      </a>
      <a
        href="/admin/exports"
        class="list-group-item list-group-item-action list-group-item-dark p-3 ps-5 <%= (typeof activePage !== 'undefined' && activePage === 'view_exports') ? 'active' : '' %>"
      >
        <i class="fas fa-list-alt me-2"></i>View Exports
      </a>
    </div>

    <!-- Events Submenu -->
    <a
      class="list-group-item list-group-item-action list-group-item-dark p-3 d-flex justify-content-between align-items-center"
      data-bs-toggle="collapse"
      href="#eventSubmenu"
      role="button"
      aria-expanded="<%= (typeof activePage !== 'undefined' && (activePage === 'add_event' || activePage === 'view_events' || activePage === 'edit_event')) ? 'true' : 'false' %>"
      aria-controls="eventSubmenu"
    >
      <span><i class="fas fa-calendar-alt me-2"></i>Events</span>
      <i class="fas fa-chevron-down"></i>
    </a>
    <div
      class="collapse <%= (typeof activePage !== 'undefined' && (activePage === 'add_event' || activePage === 'view_events' || activePage === 'edit_event')) ? 'show' : '' %>"
      id="eventSubmenu"
    >
      <a
        href="/admin/events/add"
        class="list-group-item list-group-item-action list-group-item-dark p-3 ps-5 <%= (typeof activePage !== 'undefined' && activePage === 'add_event') ? 'active' : '' %>"
      >
        <i class="fas fa-plus-circle me-2"></i>Add Event
      </a>
      <a
        href="/admin/events"
        class="list-group-item list-group-item-action list-group-item-dark p-3 ps-5 <%= (typeof activePage !== 'undefined' && activePage === 'view_events') ? 'active' : '' %>"
      >
        <i class="fas fa-list-alt me-2"></i>View Events
      </a>
    </div>
    <!-- End Events Submenu -->

    <a
      href="/admin/industrialservices"
      class="list-group-item list-group-item-action list-group-item-dark p-3 <%= (typeof activePage !== 'undefined' && (activePage === 'view_services' || activePage === 'add_service' || activePage === 'edit_service')) ? 'active' : '' %>"
    >
      <i class="fas fa-industry me-2"></i>Industrial Services
    </a>

    <!-- Consultations Submenu -->
    <a
      class="list-group-item list-group-item-action list-group-item-dark p-3 d-flex justify-content-between align-items-center"
      data-bs-toggle="collapse"
      href="#consultationSubmenu"
      role="button"
      aria-expanded="<%= (typeof activePage !== 'undefined' && (activePage === 'add_consultation' || activePage === 'view_consultations' || activePage === 'edit_consultation')) ? 'true' : 'false' %>"
      aria-controls="consultationSubmenu"
    >
      <span><i class="fas fa-comments me-2"></i>Consultations</span>
      <i class="fas fa-chevron-down"></i>
    </a>
    <div
      class="collapse <%= (typeof activePage !== 'undefined' && (activePage === 'add_consultation' || activePage === 'view_consultations' || activePage === 'edit_consultation')) ? 'show' : '' %>"
      id="consultationSubmenu"
    >
      <a
        href="/admin/consultations/add"
        class="list-group-item list-group-item-action list-group-item-dark p-3 ps-5 <%= (typeof activePage !== 'undefined' && activePage === 'add_consultation') ? 'active' : '' %>"
      >
        <i class="fas fa-plus-circle me-2"></i>Add Consultation
      </a>
      <a
        href="/admin/consultations"
        class="list-group-item list-group-item-action list-group-item-dark p-3 ps-5 <%= (typeof activePage !== 'undefined' && activePage === 'view_consultations') ? 'active' : '' %>"
      >
        <i class="fas fa-list-alt me-2"></i>View Consultations
      </a>
    </div>
    <!-- End Consultations Submenu -->

    <!-- Careers Submenu (Job Postings) -->
    <a
      class="list-group-item list-group-item-action list-group-item-dark p-3 d-flex justify-content-between align-items-center"
      data-bs-toggle="collapse"
      href="#careerSubmenu"
      role="button"
      aria-expanded="<%= (typeof activePage !== 'undefined' && (activePage === 'add_career' || activePage === 'view_careers' || activePage === 'edit_career')) ? 'true' : 'false' %>"
      aria-controls="careerSubmenu"
